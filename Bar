class Bar {
  constructor (index, previousHash, timestamp, data, provider, hash, nonce) {
    this.index = index;
    this.previousHash = previousHash;
    this.timestamp = timestamp;
    this.data = data;
    this.provider = provider;
    this.hash = hash;
    this.nonce = nonce;
  }

  get genesis() {
    new Bar(
      0,
      "0",
      1508270000000,
      "Data",
      "Hash",
      604
    );
  }
}

module.exports = Block;
